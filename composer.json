{
    "name": "qobo/cakephp-translations",
    "description": "Languages and translations plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "admad/cakephp-jwt-auth": "^2.0",
        "friendsofcake/crud": "^4.3",
        "qobo/cakephp-utils": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "cakephp/cakephp-codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Translations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Translations\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
